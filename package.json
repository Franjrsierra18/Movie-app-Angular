    "@angular/material": "^12.2.13",
    "@angular/platform-browser": "^13.3.1",
    "@angular/platform-browser-dynamic": "^13.3.1",
    "@angular/router": "^13.3.1",
    "bootstrap": "4.1.1",
    "ng2-search-filter": "^0.5.1",
    "ngx-bootstrap": "^5.6.2",
    "rxjs": "~6.6.7",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
